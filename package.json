{
  "name": "blessed-dash",
  "version": "1.5.0",
  "description": "A high-level terminal interface library for `NodeJS`",
  "author": "PatEvs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patevs/blessed-dash.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "release": "np",
    "test": "echo \"Error: no test specified\" && exit 0",
    "updates": "script-progress node ./bin/checkUpdates"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "chalk": "^4.1.0",
    "commitlint-config-gitmoji": "^1.0.1",
    "husky": "^4.3.6",
    "np": "^7.0.0",
    "script-progress": "^1.0.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": ["./node_modules/commitlint-config-gitmoji"]
  }
}
