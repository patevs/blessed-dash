{
  "name": "blessed-dash",
  "version": "1.6.0",
  "description": "A high-level terminal interface library for `NodeJS`",
  "author": "PatEvs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patevs/blessed-dash.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "updates": "script-progress node ./bin/checkUpdates",
    "release": "np"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "chalk": "^4.1.1",
    "commitlint-config-gitmoji": "^2.2.5",
    "husky": "^4.3.8",
    "np": "^7.5.0",
    "script-progress": "^1.0.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "./node_modules/commitlint-config-gitmoji"
    ]
  }
}
