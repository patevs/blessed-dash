{
  "name": "blessed-dash",
  "version": "2.2.0",
  "description": "A high-level terminal interface library for `NodeJS`",
  "author": "PatEvs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patevs/blessed-dash.git"
  },
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "node index.js",
    "updates": "script-progress node ./bin/checkUpdates",
    "release": "np"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "chalk": "^5.1.2",
    "commitlint-config-gitmoji": "^2.2.6",
    "husky": "^7.0.4",
    "np": "^7.6.1",
    "script-progress": "^1.0.5"
  },
  "commitlint": {
    "extends": [
      "gitmoji"
    ],
    "rules": {
      "subject-empty": [
        0,
        "never"
      ],
      "type-empty": [
        0,
        "never"
      ]
    }
  }
}
